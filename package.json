{
  "name": "cypress-example-module-api",
  "version": "1.0.0",
  "description": "Example running specs using Cypress via its module API",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "mocha --timeout 30000 test/spec.js",
    "lint": "standard --verbose --fix test/*.js",
    "cy:verify": "cypress verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypress-io/cypress-example-module-api.git"
  },
  "keywords": [
    "cypress",
    "cypress-io",
    "cypress-test"
  ],
  "author": "Gleb Bahmutov <gleb@cypress.io>",
  "private": true,
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cypress-io/cypress-example-module-api/issues"
  },
  "homepage": "https://github.com/cypress-io/cypress-example-module-api#readme",
  "devDependencies": {
    "@bahmutov/all-paths": "1.0.2",
    "chdir-promise": "0.6.2",
    "check-more-types": "2.24.0",
    "debug": "4.1.1",
    "lazy-ass": "1.6.0",
    "mocha": "6.2.2",
    "mocha-banner": "1.1.2",
    "ramda": "0.26.1",
    "snap-shot-it": "5.0.1",
    "standard": "12.0.1",
    "terminal-banner": "1.1.0"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "it",
      "afterEach"
    ]
  }
}
